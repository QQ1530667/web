{% extends "block.html.twig" %}
{#
/**
 * @file
 * Theme override for tabs.
 */
#}
{% block content %}
    {% if content %}
        <div{{ attributes.addClass('bb4-tabs clearfix')  }}>
            <nav class="tabs" role="navigation" aria-label="{{ 'Tabs'|t }}">
                {{ content }}
            </nav>
        </div>
    {% endif %}
{% endblock %}
